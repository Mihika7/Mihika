@bot.on(events.InlineQuery(pattern=r"repo"))
async def inline_id_handler(event: events.InlineQuery.Event):
 LEGEND = event.builder
 X= [[custom.Button.inline("ðŸ”¥ CLICK ME ðŸ”¥",data="obhai")]]
